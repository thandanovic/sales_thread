<%# Page Header %>
<%= render "shared/components/page_header",
    title: "Products",
    subtitle: "Manage your shop products",
    actions: capture {
      concat link_to(new_shop_import_path(@shop), class: "inline-flex items-center gap-2 rounded-lg border border-gray-300 bg-white px-4 py-2.5 text-sm font-medium text-gray-700 hover:bg-gray-50 dark:border-strokedark dark:bg-boxdark dark:text-bodydark1 dark:hover:bg-strokedark transition-colors") {
        raw('<svg class="h-4 w-4" fill="none" stroke="currentColor" viewBox="0 0 24 24"><path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M7 16a4 4 0 01-.88-7.903A5 5 0 1115.9 6L16 6a5 5 0 011 9.9M15 13l-3-3m0 0l-3 3m3-3v12"/></svg>') +
        "Import"
      }
      concat " "
      concat link_to(new_shop_product_path(@shop), class: "inline-flex items-center gap-2 rounded-lg bg-primary px-5 py-2.5 text-sm font-medium text-white hover:bg-primary-dark transition-colors") {
        raw('<svg class="h-4 w-4" fill="none" stroke="currentColor" viewBox="0 0 24 24"><path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M12 4v16m8-8H4"/></svg>') +
        "New Product"
      }
    } %>

<% if @products.any? || params[:olx_status].present? || params[:template_id].present? || params[:search].present? %>
  <%# Filters Card %>
  <%= render "shared/components/card", title: "Filters" do %>
    <%= form_with url: shop_products_path(@shop), method: :get, class: "flex flex-wrap items-end gap-4" do |f| %>
      <div class="flex-1 min-w-[250px]">
        <label class="mb-2 block text-sm font-medium text-gray-700 dark:text-bodydark1">Search</label>
        <div class="flex items-center rounded-lg border border-gray-300 bg-white dark:border-strokedark dark:bg-boxdark focus-within:border-primary focus-within:ring-1 focus-within:ring-primary">
          <%= f.text_field :search,
              value: params[:search],
              placeholder: "Search title, models, description, OLX ID...",
              class: "w-full bg-transparent py-2.5 pl-4 pr-2 text-sm text-gray-900 outline-none dark:text-white dark:placeholder-gray-400" %>
          <span class="pr-3">
            <svg class="h-5 w-5 text-gray-400" fill="none" stroke="currentColor" viewBox="0 0 24 24">
              <path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M21 21l-6-6m2-5a7 7 0 11-14 0 7 7 0 0114 0z"/>
            </svg>
          </span>
        </div>
      </div>

      <div class="flex-1 min-w-[180px]">
        <label class="mb-2 block text-sm font-medium text-gray-700 dark:text-bodydark1">OLX Status</label>
        <%= f.select :olx_status,
            options_for_select([
              ['All', ''],
              ['Live', 'live'],
              ['Draft', 'draft'],
              ['Not on OLX', 'not_on_olx']
            ], params[:olx_status]),
            {},
            class: "w-full rounded-lg border border-gray-300 bg-white py-2.5 pl-4 pr-10 text-sm text-gray-900 outline-none focus:border-primary focus:ring-1 focus:ring-primary dark:border-strokedark dark:bg-boxdark dark:text-white appearance-none cursor-pointer" %>
      </div>

      <div class="flex-1 min-w-[180px]">
        <label class="mb-2 block text-sm font-medium text-gray-700 dark:text-bodydark1">Template</label>
        <%= f.select :template_id,
            options_for_select([['All Templates', '']] + @templates.map { |t| [t.display_name, t.id] }, params[:template_id]),
            {},
            { data: { controller: "slim-select", slim_select_placeholder_value: "All Templates", slim_select_search_placeholder_value: "Search templates..." } } %>
      </div>

      <div class="min-w-[120px]">
        <label class="mb-2 block text-sm font-medium text-gray-700 dark:text-bodydark1">Per Page</label>
        <%= f.select :per_page,
            options_for_select([['50', 50], ['100', 100], ['500', 500], ['1000', 1000]], @per_page),
            {},
            class: "w-full rounded-lg border border-gray-300 bg-white py-2.5 pl-4 pr-10 text-sm text-gray-900 outline-none focus:border-primary focus:ring-1 focus:ring-primary dark:border-strokedark dark:bg-boxdark dark:text-white appearance-none cursor-pointer" %>
      </div>

      <div class="flex gap-2">
        <%= f.submit "Apply", class: "inline-flex items-center gap-2 rounded-lg bg-primary px-5 py-2.5 text-sm font-medium text-white hover:bg-primary-dark transition-colors cursor-pointer" %>
        <%= link_to "Clear", shop_products_path(@shop), class: "inline-flex items-center gap-2 rounded-lg border border-gray-300 bg-white px-5 py-2.5 text-sm font-medium text-gray-700 hover:bg-gray-50 dark:border-strokedark dark:bg-boxdark dark:text-bodydark1 dark:hover:bg-strokedark transition-colors" %>
      </div>
    <% end %>
  <% end %>

  <% if @products.any? %>
    <%# Bulk Actions Card %>
    <div class="mt-6 rounded-xl border border-gray-200 bg-white dark:border-strokedark dark:bg-boxdark">
      <div class="border-b border-gray-200 px-6 py-4 dark:border-strokedark">
        <div class="flex items-center justify-between">
          <h3 class="text-lg font-semibold text-gray-900 dark:text-white">Bulk Actions</h3>
          <div class="flex items-center gap-2">
            <button type="button" id="select-all-btn" class="text-sm text-primary hover:text-primary-dark">Select All</button>
            <span class="text-gray-300">|</span>
            <button type="button" id="deselect-all-btn" class="text-sm text-gray-500 hover:text-gray-700 dark:text-bodydark2">Deselect All</button>
          </div>
        </div>
      </div>

      <div class="p-6 space-y-4">
        <%# Margin Update %>
        <%= form_with url: bulk_update_margin_shop_products_path(@shop), method: :post, id: "bulk-margin-form" do |f| %>
          <div class="flex items-center justify-between gap-4 rounded-lg bg-info/10 p-4">
            <div class="flex items-center gap-3">
              <div class="flex h-10 w-10 items-center justify-center rounded-full bg-info/20">
                <svg class="h-5 w-5 text-info" fill="none" stroke="currentColor" viewBox="0 0 24 24">
                  <path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M12 8c-1.657 0-3 .895-3 2s1.343 2 3 2 3 .895 3 2-1.343 2-3 2m0-8c1.11 0 2.08.402 2.599 1M12 8V7m0 1v8m0 0v1m0-1c-1.11 0-2.08-.402-2.599-1M21 12a9 9 0 11-18 0 9 9 0 0118 0z"/>
                </svg>
              </div>
              <span class="font-medium text-gray-900 dark:text-white">Update Margin</span>
            </div>
            <div class="flex items-center gap-3">
              <%= f.number_field :margin, step: 0.01, placeholder: "15.50", class: "w-24 rounded-lg border border-gray-300 bg-white px-3 py-2 text-sm focus:border-primary focus:ring-primary dark:border-strokedark dark:bg-boxdark" %>
              <span class="text-sm text-gray-500">%</span>
              <%= f.submit "Update Selected", class: "rounded-lg bg-info px-4 py-2 text-sm font-medium text-white hover:bg-info/90 transition-colors cursor-pointer" %>
            </div>
          </div>
        <% end %>

        <%# OLX Operations %>
        <div class="flex items-center justify-between gap-4 rounded-lg bg-success/10 p-4">
          <div class="flex items-center gap-3">
            <div class="flex h-10 w-10 items-center justify-center rounded-full bg-success/20">
              <svg class="h-5 w-5 text-success" fill="none" stroke="currentColor" viewBox="0 0 24 24">
                <path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M7 16a4 4 0 01-.88-7.903A5 5 0 1115.9 6L16 6a5 5 0 011 9.9M15 13l-3-3m0 0l-3 3m3-3v12"/>
              </svg>
            </div>
            <span class="font-medium text-gray-900 dark:text-white">OLX Operations</span>
          </div>
          <div class="flex items-center gap-2">
            <form action="<%= bulk_publish_to_olx_shop_products_path(@shop) %>" method="post" id="bulk-publish-form" class="inline">
              <%= hidden_field_tag :authenticity_token, form_authenticity_token %>
              <button type="submit" class="inline-flex items-center gap-1.5 rounded-lg bg-success px-4 py-2 text-sm font-medium text-white hover:bg-success/90 transition-colors">
                <svg class="h-4 w-4" fill="none" stroke="currentColor" viewBox="0 0 24 24">
                  <path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M5 13l4 4L19 7"/>
                </svg>
                Publish
              </button>
            </form>

            <form action="<%= bulk_update_on_olx_shop_products_path(@shop) %>" method="post" id="bulk-update-form" class="inline">
              <%= hidden_field_tag :authenticity_token, form_authenticity_token %>
              <button type="submit" class="inline-flex items-center gap-1.5 rounded-lg border border-success bg-white px-4 py-2 text-sm font-medium text-success hover:bg-success/10 dark:bg-transparent transition-colors">
                <svg class="h-4 w-4" fill="none" stroke="currentColor" viewBox="0 0 24 24">
                  <path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M4 4v5h.582m15.356 2A8.001 8.001 0 004.582 9m0 0H9m11 11v-5h-.581m0 0a8.003 8.003 0 01-15.357-2m15.357 2H15"/>
                </svg>
                Update
              </button>
            </form>

            <form action="<%= bulk_remove_from_olx_shop_products_path(@shop) %>" method="post" id="bulk-remove-form" class="inline" data-turbo-confirm="Are you sure? This will remove the selected products from OLX.">
              <%= hidden_field_tag :authenticity_token, form_authenticity_token %>
              <%= hidden_field_tag :_method, :delete %>
              <button type="submit" class="inline-flex items-center gap-1.5 rounded-lg border border-gray-300 bg-white px-4 py-2 text-sm font-medium text-gray-700 hover:bg-gray-50 dark:border-strokedark dark:bg-boxdark dark:text-bodydark1 transition-colors">
                <svg class="h-4 w-4" fill="none" stroke="currentColor" viewBox="0 0 24 24">
                  <path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M10 14l2-2m0 0l2-2m-2 2l-2-2m2 2l2 2m7-2a9 9 0 11-18 0 9 9 0 0118 0z"/>
                </svg>
                Remove
              </button>
            </form>

            <form action="<%= bulk_disconnect_from_olx_shop_products_path(@shop) %>" method="post" id="bulk-disconnect-form" class="inline" data-turbo-confirm="Are you sure? This will disconnect the selected products from OLX (local records only). Listings on OLX will NOT be affected.">
              <%= hidden_field_tag :authenticity_token, form_authenticity_token %>
              <%= hidden_field_tag :_method, :delete %>
              <button type="submit" class="inline-flex items-center gap-1.5 rounded-lg border border-gray-300 bg-white px-4 py-2 text-sm font-medium text-gray-700 hover:bg-gray-50 dark:border-strokedark dark:bg-boxdark dark:text-bodydark1 transition-colors" title="Disconnect from OLX without removing listings">
                <svg class="h-4 w-4" fill="none" stroke="currentColor" viewBox="0 0 24 24">
                  <path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M13.828 10.172a4 4 0 00-5.656 0l-4 4a4 4 0 105.656 5.656l1.102-1.101m-.758-4.899a4 4 0 005.656 0l4-4a4 4 0 00-5.656-5.656l-1.1 1.1"/>
                </svg>
                Disconnect
              </button>
            </form>
          </div>
        </div>

        <%# Delete %>
        <%= form_with url: bulk_destroy_shop_products_path(@shop), method: :delete, id: "bulk-delete-form" do |f| %>
          <div class="flex items-center justify-between gap-4 rounded-lg bg-danger/10 p-4">
            <div class="flex items-center gap-3">
              <div class="flex h-10 w-10 items-center justify-center rounded-full bg-danger/20">
                <svg class="h-5 w-5 text-danger" fill="none" stroke="currentColor" viewBox="0 0 24 24">
                  <path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M19 7l-.867 12.142A2 2 0 0116.138 21H7.862a2 2 0 01-1.995-1.858L5 7m5 4v6m4-6v6m1-10V4a1 1 0 00-1-1h-4a1 1 0 00-1 1v3M4 7h16"/>
                </svg>
              </div>
              <span class="font-medium text-gray-900 dark:text-white">Delete Products</span>
            </div>
            <%= f.submit "Delete Selected", class: "rounded-lg bg-danger px-4 py-2 text-sm font-medium text-white hover:bg-danger/90 transition-colors cursor-pointer" %>
          </div>
        <% end %>
      </div>
    </div>

    <%# Products Table %>
    <div class="mt-6 rounded-xl border border-gray-200 bg-white shadow-card dark:border-strokedark dark:bg-boxdark">
      <div class="overflow-x-auto">
        <table class="w-full table-auto">
          <thead>
            <tr class="border-b border-gray-200 bg-gray-50 dark:border-strokedark dark:bg-boxdark-2">
              <th class="w-12 px-4 py-4">
                <input type="checkbox" id="select-all-checkbox" class="h-5 w-5 rounded border-gray-300 text-primary focus:ring-primary">
              </th>
              <th class="px-4 py-4 text-left text-sm font-medium text-gray-600 dark:text-bodydark2">OLX</th>
              <th class="px-4 py-4 text-left text-sm font-medium text-gray-600 dark:text-bodydark2">Product</th>
              <th class="px-4 py-4 text-left text-sm font-medium text-gray-600 dark:text-bodydark2">SKU</th>
              <th class="px-4 py-4 text-left text-sm font-medium text-gray-600 dark:text-bodydark2">Price</th>
              <th class="px-4 py-4 text-left text-sm font-medium text-gray-600 dark:text-bodydark2">Final</th>
              <th class="px-4 py-4 text-left text-sm font-medium text-gray-600 dark:text-bodydark2">Actions</th>
            </tr>
          </thead>
          <tbody>
            <% @products.each do |product| %>
              <tr class="border-b border-gray-200 hover:bg-gray-50 dark:border-strokedark dark:hover:bg-boxdark-2 transition-colors">
                <td class="px-4 py-4">
                  <input type="checkbox" name="product_ids[]" value="<%= product.id %>" class="product-checkbox h-5 w-5 rounded border-gray-300 text-primary focus:ring-primary" data-has-olx="<%= product.has_olx_listing? %>">
                </td>
                <td class="px-4 py-4">
                  <% if product.published_on_olx? %>
                    <%= render "shared/components/badge", text: "Live", type: "success" %>
                  <% elsif product.has_olx_listing? %>
                    <%= render "shared/components/badge", text: "Draft", type: "warning" %>
                  <% else %>
                    <%= render "shared/components/badge", text: "Not on OLX", type: "default" %>
                  <% end %>
                </td>
                <td class="px-4 py-4">
                  <div class="flex items-center gap-3">
                    <% if product.images.attached? && product.images.first.present? %>
                      <%= image_tag product.images.first, class: "h-12 w-12 rounded-lg object-cover" %>
                    <% elsif product.image_urls.present? && product.image_urls.first.present? %>
                      <%= image_tag product.image_urls.first, class: "h-12 w-12 rounded-lg object-cover" %>
                    <% else %>
                      <div class="flex h-12 w-12 items-center justify-center rounded-lg bg-gray-100 dark:bg-strokedark">
                        <svg class="h-6 w-6 text-gray-400" fill="none" stroke="currentColor" viewBox="0 0 24 24">
                          <path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M4 16l4.586-4.586a2 2 0 012.828 0L16 16m-2-2l1.586-1.586a2 2 0 012.828 0L20 14m-6-6h.01M6 20h12a2 2 0 002-2V6a2 2 0 00-2-2H6a2 2 0 00-2 2v12a2 2 0 002 2z"/>
                        </svg>
                      </div>
                    <% end %>
                    <div class="min-w-0">
                      <%= link_to product.title, shop_product_path(@shop, product), class: "block font-medium text-gray-900 hover:text-primary dark:text-white truncate max-w-[200px]" %>
                      <% if product.brand.present? %>
                        <p class="text-sm text-gray-500 dark:text-bodydark2"><%= product.brand %></p>
                      <% end %>
                    </div>
                  </div>
                </td>
                <td class="px-4 py-4">
                  <span class="text-sm text-gray-600 dark:text-bodydark2"><%= product.sku || '-' %></span>
                </td>
                <td class="px-4 py-4">
                  <span class="text-sm text-gray-900 dark:text-white"><%= number_to_currency(product.price || 0, unit: product.currency || "BAM") %></span>
                </td>
                <td class="px-4 py-4">
                  <span class="text-sm font-semibold text-success"><%= number_to_currency(product.final_price || 0, unit: product.currency || "BAM") %></span>
                </td>
                <td class="px-4 py-4">
                  <div class="flex items-center gap-2">
                    <%= link_to edit_shop_product_path(@shop, product), class: "flex h-8 w-8 items-center justify-center rounded-lg hover:bg-gray-100 dark:hover:bg-strokedark transition-colors", title: "Edit" do %>
                      <svg class="h-4 w-4 text-gray-500" fill="none" stroke="currentColor" viewBox="0 0 24 24">
                        <path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M11 5H6a2 2 0 00-2 2v11a2 2 0 002 2h11a2 2 0 002-2v-5m-1.414-9.414a2 2 0 112.828 2.828L11.828 15H9v-2.828l8.586-8.586z"/>
                      </svg>
                    <% end %>

                    <%= form_with model: [product.shop, product], method: :patch, local: true, class: "flex items-center gap-1" do |f| %>
                      <%= f.number_field :margin, value: product.margin, step: 0.01, placeholder: "%", class: "w-16 rounded-lg border border-gray-300 px-2 py-1 text-sm focus:border-primary focus:ring-primary dark:border-strokedark dark:bg-boxdark" %>
                      <%= f.submit "✓", class: "flex h-7 w-7 items-center justify-center rounded-lg bg-primary text-white text-xs hover:bg-primary-dark cursor-pointer" %>
                    <% end %>
                  </div>
                </td>
              </tr>
            <% end %>
          </tbody>
        </table>
      </div>
    </div>

    <%# Pagination %>
    <div class="mt-6">
      <%= paginate @products %>
    </div>

    <%# JavaScript for bulk selection %>
    <script>
      document.addEventListener('turbo:load', function() {
        const selectAllCheckbox = document.getElementById('select-all-checkbox');
        const selectAllBtn = document.getElementById('select-all-btn');
        const deselectAllBtn = document.getElementById('deselect-all-btn');
        const productCheckboxes = document.querySelectorAll('.product-checkbox');
        const bulkForms = ['bulk-margin-form', 'bulk-delete-form', 'bulk-publish-form', 'bulk-update-form', 'bulk-remove-form', 'bulk-disconnect-form'];

        // Select all checkbox
        if (selectAllCheckbox) {
          selectAllCheckbox.addEventListener('change', function() {
            productCheckboxes.forEach(cb => cb.checked = this.checked);
          });
        }

        // Select all button
        if (selectAllBtn) {
          selectAllBtn.addEventListener('click', function() {
            productCheckboxes.forEach(cb => cb.checked = true);
            if (selectAllCheckbox) selectAllCheckbox.checked = true;
          });
        }

        // Deselect all button
        if (deselectAllBtn) {
          deselectAllBtn.addEventListener('click', function() {
            productCheckboxes.forEach(cb => cb.checked = false);
            if (selectAllCheckbox) selectAllCheckbox.checked = false;
          });
        }

        // Update select all when individual checkboxes change
        productCheckboxes.forEach(cb => {
          cb.addEventListener('change', function() {
            if (selectAllCheckbox) {
              selectAllCheckbox.checked = Array.from(productCheckboxes).every(cb => cb.checked);
            }
          });
        });

        // Copy product IDs to forms on submit
        bulkForms.forEach(formId => {
          const form = document.getElementById(formId);
          if (form) {
            form.addEventListener('submit', function(e) {
              // Remove existing hidden inputs
              form.querySelectorAll('input[name="product_ids[]"]').forEach(i => i.remove());

              // Add checked product IDs and count OLX products
              let hasChecked = false;
              let olxProductCount = 0;
              let totalSelected = 0;

              productCheckboxes.forEach(cb => {
                if (cb.checked) {
                  hasChecked = true;
                  totalSelected++;
                  if (cb.dataset.hasOlx === 'true') {
                    olxProductCount++;
                  }
                  const input = document.createElement('input');
                  input.type = 'hidden';
                  input.name = 'product_ids[]';
                  input.value = cb.value;
                  form.appendChild(input);
                }
              });

              if (!hasChecked) {
                e.preventDefault();
                alert('Please select at least one product.');
                return;
              }

              // Special confirmation for bulk delete
              if (formId === 'bulk-delete-form') {
                let confirmMessage = `Are you sure you want to delete ${totalSelected} product(s)?\n\nThis action cannot be undone.`;

                if (olxProductCount > 0) {
                  confirmMessage = `Are you sure you want to delete ${totalSelected} product(s)?\n\n⚠️ WARNING: ${olxProductCount} product(s) are published on OLX and will be REMOVED from OLX as well.\n\nThis action cannot be undone.`;
                }

                if (!confirm(confirmMessage)) {
                  e.preventDefault();
                }
              }
            });
          }
        });
      });
    </script>
  <% else %>
    <%# No products match filters %>
    <div class="mt-6 rounded-xl border border-gray-200 bg-white p-12 text-center shadow-card dark:border-strokedark dark:bg-boxdark">
      <div class="mx-auto flex h-16 w-16 items-center justify-center rounded-full bg-gray-100 dark:bg-strokedark">
        <svg class="h-8 w-8 text-gray-400" fill="none" stroke="currentColor" viewBox="0 0 24 24">
          <path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M21 21l-6-6m2-5a7 7 0 11-14 0 7 7 0 0114 0z"/>
        </svg>
      </div>
      <h3 class="mt-4 text-lg font-semibold text-gray-900 dark:text-white">No products match your filters</h3>
      <p class="mt-2 text-sm text-gray-500 dark:text-bodydark2">Try adjusting your filters or clear them to see all products.</p>
      <div class="mt-6">
        <%= link_to shop_products_path(@shop), class: "inline-flex items-center gap-2 rounded-lg bg-primary px-5 py-2.5 text-sm font-medium text-white hover:bg-primary-dark" do %>
          Clear Filters
        <% end %>
      </div>
    </div>
  <% end %>
<% else %>
  <%# Empty State - No products at all %>
  <div class="rounded-xl border border-gray-200 bg-white p-12 text-center shadow-card dark:border-strokedark dark:bg-boxdark">
    <div class="mx-auto flex h-16 w-16 items-center justify-center rounded-full bg-gray-100 dark:bg-strokedark">
      <svg class="h-8 w-8 text-gray-400" fill="none" stroke="currentColor" viewBox="0 0 24 24">
        <path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M20 7l-8-4-8 4m16 0l-8 4m8-4v10l-8 4m0-10L4 7m8 4v10M4 7v10l8 4"/>
      </svg>
    </div>
    <h3 class="mt-4 text-lg font-semibold text-gray-900 dark:text-white">No products yet</h3>
    <p class="mt-2 text-sm text-gray-500 dark:text-bodydark2">Get started by adding a product or importing from CSV/Intercars.</p>
    <div class="mt-6 flex justify-center gap-3">
      <%= link_to new_shop_product_path(@shop), class: "inline-flex items-center gap-2 rounded-lg bg-primary px-5 py-2.5 text-sm font-medium text-white hover:bg-primary-dark" do %>
        <svg class="h-5 w-5" fill="none" stroke="currentColor" viewBox="0 0 24 24">
          <path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M12 4v16m8-8H4"/>
        </svg>
        New Product
      <% end %>
      <%= link_to new_shop_import_path(@shop), class: "inline-flex items-center gap-2 rounded-lg border border-gray-300 bg-white px-5 py-2.5 text-sm font-medium text-gray-700 hover:bg-gray-50 dark:border-strokedark dark:bg-boxdark dark:text-bodydark1 dark:hover:bg-strokedark" do %>
